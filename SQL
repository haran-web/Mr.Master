CREATE DATABASE mrmaster;
USE mrmaster;

CREATE TABLE admins (
    id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(255) NOT NULL,
    password VARCHAR(255) NOT NULL
);

CREATE TABLE users (
    id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(255) NOT NULL,
    password VARCHAR(255) NOT NULL,
    email VARCHAR(255),
    ip_address VARCHAR(50)
);

CREATE TABLE papers (
    id INT AUTO_INCREMENT PRIMARY KEY,
    grade VARCHAR(10),
    file_name VARCHAR(255),
    file_size INT,
    file_path VARCHAR(255)
);
